package project.root.main { 		import flash.display.MovieClip;	import flash.events.Event;		import core.InitProject;	import com.greensock.loading.data.XMLLoaderVars;	import com.greensock.loading.XMLLoader;	import com.greensock.events.LoaderEvent;		public class MainSWF extends MovieClip {						public function MainSWF() {									if (stage == null) stop();			else new InitProject(this);						new Config();						new ConfigData();			addEventListener(Event.ADDED_TO_STAGE, added);					}				public function added(e:Event) {						removeEventListener(Event.ADDED_TO_STAGE, added);					}				public function handleXML() {						var xmlLoaderVars = new XMLLoaderVars().onComplete(_onXMLComplete).onProgress(_onXMLProgress).onIOError(_xmlError).onError(_xmlError);						var xmlLoader:XMLLoader = new XMLLoader(Config.XML_FILE, xmlLoaderVars);			xmlLoader.load();		}				private function _xmlError(e:LoaderEvent) {						Config.xmlLoaded = false;						gotoAndPlay("go");					}				private function _onXMLComplete(e:LoaderEvent) {						Config.xmlLoaded = true;						gotoAndPlay("go");					}				private function _onXMLProgress(e:LoaderEvent) {						//Trace.add(e.target.progress);					}					}		}