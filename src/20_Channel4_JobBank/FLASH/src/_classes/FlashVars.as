package {	/*  *  * 	var fv = "xmlHOF"; *  if (FlashVars.get(fv)) xmlFile = FlashVars.get(fv); *  *  * */	public class FlashVars	{				public static var TRACE:Boolean = true;				public static function get(what)		{			var fv = getFV(what);						if (fv == undefined) {							if (TRACE)			Trace.add("Check your HTML code. FlashVar \"" + what + "\" isn't defined!"); 						return false; } else return fv;								}				public static function getFV(what) {						//return Stager.getStage().loaderInfo.parameters[what];					}				public static function getFVDefault(what, defaultValue) {						var fv = getFV(what);						if (fv == undefined) fv = defaultValue;						return fv;					}			}	}