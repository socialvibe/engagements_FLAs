package mirrorbanners.fx.dust {	import flash.display.MovieClip;	import flash.events.Event;		public class Pixie extends MovieClip	{				private const EASE					:Number = 0.2;				protected var _targX				:Number;		protected var _targY				:Number;						public function Pixie()		{			//blendMode = "add";			//alpha = 0.8;			addEventListener( Event.ENTER_FRAME, onFrameEnter );			addEventListener( Event.REMOVED_FROM_STAGE, onStageRemoved );		}				public function moveTo( $x:Number, $y:Number ):void		{			_targX = $x;			_targY = $y;		}				protected function onFrameEnter( e:Event ):void		{			//x -= ( x - _targX ) * EASE;			//y -= ( y - _targY ) * EASE;			//scaleX = scaleY = Math.random() * 0.07 + 0.5;		}				protected function onStageRemoved( e:Event ):void		{			removeEventListener( Event.ENTER_FRAME, onFrameEnter );		}	}	}